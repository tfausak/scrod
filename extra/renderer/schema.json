{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://scrod.fyi/schema.json",
    "title": "Scrod",
    "description": "JSON output of the Scrod Haskell documentation tool.",
    "type": "object",
    "properties": {
        "version": {
            "type": "array",
            "items": {
                "type": "integer",
                "minimum": 0
            },
            "minItems": 1
        },
        "language": {
            "type": "string"
        },
        "extensions": {
            "type": "object",
            "additionalProperties": {
                "type": "boolean"
            }
        },
        "documentation": {
            "$ref": "#/$defs/doc"
        },
        "since": {
            "$ref": "#/$defs/since"
        },
        "signature": {
            "type": "boolean"
        },
        "name": {
            "$ref": "#/$defs/located"
        },
        "warning": {
            "$ref": "#/$defs/warning"
        },
        "exports": {
            "type": "array",
            "items": {
                "$ref": "#/$defs/export"
            }
        },
        "imports": {
            "type": "array",
            "items": {
                "$ref": "#/$defs/import"
            }
        },
        "items": {
            "type": "array",
            "items": {
                "$ref": "#/$defs/located"
            }
        }
    },
    "required": [
        "version",
        "extensions",
        "documentation",
        "signature",
        "imports",
        "items"
    ],
    "additionalProperties": false,
    "$defs": {
        "cell": {
            "type": "object",
            "properties": {
                "colspan": {
                    "type": "integer",
                    "minimum": 0
                },
                "rowspan": {
                    "type": "integer",
                    "minimum": 0
                },
                "contents": {
                    "$ref": "#/$defs/doc"
                }
            },
            "required": [
                "colspan",
                "rowspan",
                "contents"
            ],
            "additionalProperties": false
        },
        "definition": {
            "type": "object",
            "properties": {
                "term": {
                    "$ref": "#/$defs/doc"
                },
                "definition": {
                    "$ref": "#/$defs/doc"
                }
            },
            "required": [
                "term",
                "definition"
            ],
            "additionalProperties": false
        },
        "doc": {
            "oneOf": [
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Empty"
                        }
                    },
                    "required": [
                        "type"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Append"
                        },
                        "value": {
                            "type": "array",
                            "items": {
                                "$ref": "#/$defs/doc"
                            }
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "String"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Paragraph"
                        },
                        "value": {
                            "$ref": "#/$defs/doc"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Identifier"
                        },
                        "value": {
                            "$ref": "#/$defs/identifier"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Module"
                        },
                        "value": {
                            "$ref": "#/$defs/modLink"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Emphasis"
                        },
                        "value": {
                            "$ref": "#/$defs/doc"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Monospaced"
                        },
                        "value": {
                            "$ref": "#/$defs/doc"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Bold"
                        },
                        "value": {
                            "$ref": "#/$defs/doc"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "UnorderedList"
                        },
                        "value": {
                            "type": "array",
                            "items": {
                                "$ref": "#/$defs/doc"
                            }
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "OrderedList"
                        },
                        "value": {
                            "type": "array",
                            "items": {
                                "$ref": "#/$defs/numberedItem"
                            }
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "DefList"
                        },
                        "value": {
                            "type": "array",
                            "items": {
                                "$ref": "#/$defs/definition"
                            }
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "CodeBlock"
                        },
                        "value": {
                            "$ref": "#/$defs/doc"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Hyperlink"
                        },
                        "value": {
                            "$ref": "#/$defs/hyperlink"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Pic"
                        },
                        "value": {
                            "$ref": "#/$defs/picture"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "MathInline"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "MathDisplay"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "AName"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Property"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Examples"
                        },
                        "value": {
                            "type": "array",
                            "items": {
                                "$ref": "#/$defs/example"
                            }
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Header"
                        },
                        "value": {
                            "$ref": "#/$defs/header"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Table"
                        },
                        "value": {
                            "$ref": "#/$defs/table"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                }
            ]
        },
        "example": {
            "type": "object",
            "properties": {
                "expression": {
                    "type": "string"
                },
                "result": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "expression",
                "result"
            ],
            "additionalProperties": false
        },
        "export": {
            "oneOf": [
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Identifier"
                        },
                        "value": {
                            "$ref": "#/$defs/exportIdentifier"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Group"
                        },
                        "value": {
                            "$ref": "#/$defs/header"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Doc"
                        },
                        "value": {
                            "$ref": "#/$defs/doc"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "DocNamed"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "type",
                        "value"
                    ],
                    "additionalProperties": false
                }
            ]
        },
        "exportIdentifier": {
            "type": "object",
            "properties": {
                "name": {
                    "$ref": "#/$defs/exportName"
                },
                "subordinates": {
                    "$ref": "#/$defs/subordinates"
                },
                "warning": {
                    "$ref": "#/$defs/warning"
                },
                "doc": {
                    "$ref": "#/$defs/doc"
                }
            },
            "required": [
                "name"
            ],
            "additionalProperties": false
        },
        "exportName": {
            "type": "object",
            "properties": {
                "kind": {
                    "$ref": "#/$defs/exportNameKind"
                },
                "name": {
                    "type": "string"
                }
            },
            "required": [
                "name"
            ],
            "additionalProperties": false
        },
        "exportNameKind": {
            "oneOf": [
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Pattern"
                        }
                    },
                    "required": [
                        "type"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Type"
                        }
                    },
                    "required": [
                        "type"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Module"
                        }
                    },
                    "required": [
                        "type"
                    ],
                    "additionalProperties": false
                }
            ]
        },
        "header": {
            "type": "object",
            "properties": {
                "level": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 6
                },
                "title": {
                    "$ref": "#/$defs/doc"
                }
            },
            "required": [
                "level",
                "title"
            ],
            "additionalProperties": false
        },
        "hyperlink": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string"
                },
                "label": {
                    "$ref": "#/$defs/doc"
                }
            },
            "required": [
                "url"
            ],
            "additionalProperties": false
        },
        "identifier": {
            "type": "object",
            "properties": {
                "namespace": {
                    "$ref": "#/$defs/namespace"
                },
                "value": {
                    "type": "string"
                }
            },
            "required": [
                "value"
            ],
            "additionalProperties": false
        },
        "import": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "package": {
                    "type": "string"
                },
                "alias": {
                    "type": "string"
                }
            },
            "required": [
                "name"
            ],
            "additionalProperties": false
        },
        "located": {
            "type": "object",
            "properties": {
                "location": {
                    "$ref": "#/$defs/location"
                },
                "value": {
                    "type": "string"
                }
            },
            "required": [
                "location",
                "value"
            ],
            "additionalProperties": false
        },
        "location": {
            "type": "object",
            "properties": {
                "line": {
                    "type": "integer",
                    "minimum": 0
                },
                "column": {
                    "type": "integer",
                    "minimum": 0
                }
            },
            "required": [
                "line",
                "column"
            ],
            "additionalProperties": false
        },
        "modLink": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "label": {
                    "$ref": "#/$defs/doc"
                }
            },
            "required": [
                "name"
            ],
            "additionalProperties": false
        },
        "namespace": {
            "oneOf": [
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Value"
                        }
                    },
                    "required": [
                        "type"
                    ],
                    "additionalProperties": false
                },
                {
                    "type": "object",
                    "properties": {
                        "type": {
                            "const": "Type"
                        }
                    },
                    "required": [
                        "type"
                    ],
                    "additionalProperties": false
                }
            ]
        },
        "numberedItem": {
            "type": "object",
            "properties": {
                "index": {
                    "type": "integer"
                },
                "item": {
                    "$ref": "#/$defs/doc"
                }
            },
            "required": [
                "index",
                "item"
            ],
            "additionalProperties": false
        },
        "picture": {
            "type": "object",
            "properties": {
                "uri": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                }
            },
            "required": [
                "uri"
            ],
            "additionalProperties": false
        },
        "since": {
            "type": "object",
            "properties": {
                "package": {
                    "type": "string"
                },
                "version": {
                    "type": "array",
                    "items": {
                        "type": "integer",
                        "minimum": 0
                    },
                    "minItems": 1
                }
            },
            "required": [
                "version"
            ],
            "additionalProperties": false
        },
        "subordinates": {
            "type": "object",
            "properties": {
                "wildcard": {
                    "type": "boolean"
                },
                "explicit": {
                    "type": "array",
                    "items": {
                        "$ref": "#/$defs/exportName"
                    }
                }
            },
            "required": [
                "wildcard",
                "explicit"
            ],
            "additionalProperties": false
        },
        "table": {
            "type": "object",
            "properties": {
                "headerRows": {
                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": {
                            "$ref": "#/$defs/cell"
                        }
                    }
                },
                "bodyRows": {
                    "type": "array",
                    "items": {
                        "type": "array",
                        "items": {
                            "$ref": "#/$defs/cell"
                        }
                    }
                }
            },
            "required": [
                "headerRows",
                "bodyRows"
            ],
            "additionalProperties": false
        },
        "warning": {
            "type": "object",
            "properties": {
                "category": {
                    "type": "string"
                },
                "value": {
                    "type": "string"
                }
            },
            "required": [
                "category",
                "value"
            ],
            "additionalProperties": false
        }
    }
}